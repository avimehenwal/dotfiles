	"frontmatter": {
		"prefix": "frontmatter",
		"body": [
			"---",
			"title: ${1:$TM_FILENAME_BASE}",
			"tags:",
			"- $2",
			"---",
			"",
			"# ${1:$TM_FILENAME_BASE}",
			"",
			"<TagLinks />",
			"",
			"$0",
			"",
			"",
			"<Footer />",
			"",
		],
		"description": "Frontmatter tags to markdown"
	},
	"warning": {
		"prefix": "warning",
		"body": [
			"::: warning ${1:text}",
			"${2:body}",
			":::",
			"",
			"$0"
		],
		"description": "vuepress warning"
	},
	"danger": {
		"prefix": "danger",
		"body": [
			"::: danger ${1:text}",
			"${2:body}",
			":::",
			"",
			"$0"
		],
		"description": "vuepress danger"
	},
	"tip": {
		"prefix": "tip",
		"body": [
			"::: tip ${1:text}",
			"${2:body}",
			":::",
			"",
			"$0"
		],
		"description": "vuepress tip"
	},
	"quote": {
		"prefix": "quote",
		"body": [
			"::: quote ${1}",
			"${2:your quote goes here}",
