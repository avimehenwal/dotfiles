	// https://flowchart.vuepress.ulivz.com/#usage
	"flowchart": {
		"prefix": "v-flowchart",
		"body": [
			"@flowstart ant",
			"st=>start: Start",
			"ip=>inputoutput: Inputoutput",
			"sub=>subroutine: Subroutine",
			"cond=>condition: Process?",
			"process=>operation: Process",
			"e=>end: End",
			"",
			"st->ip",
			"ip->cond",
			"cond(yes)->process->e",
			"cond(no)->sub",
			"sub->e",
			"@flowend"
		],
		"description": "flowchart"
	},
	"Pros and Cons component": {
		"prefix": "v-proscons",
		"body": [
			"",
			"<Procons>",
			"  <template v-slot:pro>",
			"    <h3>Some pros</h3>",
			"    <ol>",
			"      <li>${1:pros}</li>",
			"    </ol>",
			"  </template>",
			"",
			"  <template v-slot:con>",
			"    <h3>Some cons</h3>",
			"    <ol>",
			"      <li>${2:cons}</li>",
			"    </ol>",
			"",
			"  </template>",
			"</Procons>"
		],
		"description": "Pros and Cons component"
	},
