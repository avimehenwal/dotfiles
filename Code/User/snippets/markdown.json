	"mermaid three box": {
		"prefix": "mermaid-flowchart",
		"body": [
			"```mermaid",
			"graph ${1|LR,TD,BT,TB,RL|}",
			"A(${2:caption}):::green",
			"B(${3:caption}):::orange",
			"C(${4:caption}):::purple",
			"",
			"A --> B -- ${5:Some text} --> C${0}",
			"",
			"classDef green fill:#1f9,stroke-width:0px;",
			"classDef orange fill:#f96,stroke-width:0px;",
			"classDef yellow fill:#FFE873,stroke-width:0px;",
			"classDef blue fill:#b8d4ff,stroke-width:0px;",
			"classDef purple fill:#f9f,stroke:#333,stroke-width:0px;",
			"",
			"click A \"https://www.wikipedia.org/\" \"Click to open wikipedia\"",
			"```",
			""
		],
		"description": "mermaid three box"
	},
	"mermaid state diagram": {
		"prefix": "mermaid-statediagram",
		"body": [
			"```mermaid",
			"stateDiagram-v2",
			"A : ${1:node name}",
			"$0",
			"state ${4:Tokenization Lexical Analysis} {",
			"    [*] --> A : ${2:process1}",
			"    A --> [*] : ${3:process2}",
			"}",
			"```"
		],
		"description": "mermaid state diagram"
	},
	"mermaid class diagram": {
		"prefix": "mermaid-classdiagram",
		"body": [
			"```mermaid",
			"classDiagram",
			"  ${1:BaseClassName} <|-- ${2:SuperClass} : ${3:extends}",
			"  class ${1:BaseClassName}{",
			"    $0",
			"    +username: str",
			"    +password: str",
			"    +verify_login()",
			"  }",
			"  class ${2:SuperClass}{",
			"      +update_profile()",
			"  }",
			"link ${1:BaseClassName} \"http://www.github.com\" \"This is a tooltip for ${1:BaseClassName}\"",
			"```",
			""
		],
		"description": "mermaid class diagram"
	},
	"mermaid journey graphs": {
